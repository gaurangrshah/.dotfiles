{
  "telemetry.telemetryLevel": "off",
  "vscode_custom_css.imports": [
    "file:///Users/knowhow/dev/._config/vscode-fonts.css"
  ],
  "editor.renderControlCharacters": true,
  "editor.fontSize": 13,
  "editor.renderWhitespace": "all",
  "editor.fontFamily": "Operator Mono Italic, Fira Code",
  "editor.fontLigatures": true,
  "editor.lineHeight": 20.3,
  "editor.letterSpacing": 0.5,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "smooth",
  "editor.cursorStyle": "block",
  "editor.tabCompletion": "on",
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "workbench.fontAliasing": "auto",
  "workbench.iconTheme": "file-icons-colourless",
  "workbench.editor.labelFormat": "short",
  "workbench.colorTheme": "Tokyo Night",
  "zenMode.fullScreen": false,
  "terminal.integrated.env.osx": {},
  "editor.matchBrackets": "always",
  "files.exclude": {
    "**/.next/": true,
    "**/node_modules": true,
    "**/.history": true
  },
  "search.mode": "newEditor",
  "search.exclude": {
    "**/.next": true,
    "**/node_modules": true,
    "**/.history": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true,
    "**/.cache": true,
    "**/.git": true,
    "**/.migrations": true,
    "**/.vscode**": true
  },
  "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "markdown": "html",
    "javascript": "javascriptreact"
  },
  "local-history.exclude": [
    "**/.history/**",
    "**/.vscode**",
    "**/node_modules/**",
    "**/.next**",
    "**/.build**",
    "**/.build**",
    "**/typings/**",
    "**/out/**",
    "**/coverage",
    "**/dist/**",
    "**/build/**",
    "**/.gh-pages/**",
    "**/.cache/**",
    "**/.git/**",
    "**/.migrations/**"
  ],
  "local-history.daysLimit": 30,
  "errorLens.enabledDiagnosticLevels": ["warning", "info", "error"],
  "errorLens.enabled": true,
  "cSpell.userWords": [
    "Astro",
    "astrojs",
    "btns",
    "Chakra",
    "ciel",
    "clsx",
    "cmyk",
    "didn",
    "favorited",
    "fontsource",
    "Gaurang",
    "gaurangrshah",
    "Hashnode",
    "heroicons",
    "ianvs",
    "lucide",
    "nextjs",
    "postbuild",
    "roadmap",
    "Supa",
    "supabase",
    "superjson",
    "Swatchr",
    "tailwindcss",
    "tanstack",
    "tinycolor",
    "trpc",
    "upsert"
  ],
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false,
    "search-result": true
  },
  "gitlens.ai.experimental.provider": "openai",
  "gitlens.ai.experimental.openai.model": "gpt-4-turbo-preview",
  "gitlens.autolinks": [
    {
      "prefix": "SWT",
      "url": ""
    }
  ],
  "turboConsoleLog.delimiterInsideMessage": "|",
  "turboConsoleLog.insertEnclosingFunction": false,
  "console-ninja.featureSet": "Community",
  "git.autofetch": true,
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "workbench.activityBar.location": "hidden",
  "hediet.vscode-drawio.resizeImages": null,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.suppressSuggestions": true,
  "cody.commandCodeLenses": true,
  "cody.commandHints.enabled": true,
  "security.promptForLocalFileProtocolHandling": false,
  "cody.autocomplete.enabled": false,
  "gitlens.graph.minimap.additionalTypes": [
    "localBranches",
    "stashes",
    "remoteBranches",
    "tags"
  ]
}
